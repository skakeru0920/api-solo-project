<h1>motorcycles List</h1>
<table cellspacing="0">
	<thread>
		<th scope="col">id</th>
		<th scope="col">車名</th>
		<th scope="col">メーカー名</th>
		<th scope="col">販売価格</th>
		<th scope="col">排気量</th>
		<th></th>
		<th></th>
	</thread>
	<tbody>
		<% motorcycles.forEach(motorcycles => { %>
		<tr>
			<td><%= motorcycles.cycle_id %></td>
			<td>
				<a href="<%= motorcycles.cycle_url%>"><%= motorcycles.cycle_name%></a>
			</td>
			<td><%= motorcycles.cycle_maker_name%></td>
			<td><%= motorcycles.cycle_sell_price%></td>
			<td><%= motorcycles.cycle_displacement%></td>
			<td><a href="/motorcycle/edit/<%= motorcycles.cycle_id %>">Edit</a></td>
			<td><a href="/motorcycle/view/<%= motorcycles.cycle_id %>"> View</a></td>
		</tr>
		<% }); %>
	</tbody>
</table>
<a href="/motorcycle/new">new motorcycle</a>
